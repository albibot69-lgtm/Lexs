-- Multi-game loader
local placeId = game.PlaceId

if placeId == 123821081589134 then
    -- Game Break Your Bone
    loadstring(game:HttpGet("https://raw.githubusercontent.com/albibot69-lgtm/Lexs/refs/heads/main/BreakYourBone2.Lua"))()
elseif placeId == 2753915549,4442272183,7449423635 then
    -- Game Blox Fruit
    loadstring(game:HttpGet("https://raw.githubusercontent.com/albibot69-lgtm/Lexs/refs/heads/main/BloxFruitHub.lua"))() 
elseif placeId == 2753915549 then
    -- Game C
    loadstring(game:HttpGet("https://raw.githubusercontent.com/username/game-c-script/main.lua"))()
else
    warn("Game not supported!")
end
